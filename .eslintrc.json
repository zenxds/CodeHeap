{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "import"
  ],
  "globals": {
    "jQuery": true,
    "$": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "no-empty": 0,
    "semi": 0,
    "eol-last": 0
  }
}
